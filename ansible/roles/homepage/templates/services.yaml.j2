---
# For configuration options and examples, please see:
# https://gethomepage.dev/en/configs/services

- Infrastructure:
    - Router:
        icon: asus-router.png
        href: http://10.100.10.1
        ping: http://10.100.10.1

    - Switch:
        icon: mdi-swap-horizontal-#ADD8E6
        href: http://10.100.10.2
        ping: http://10.100.10.2

    - Unifi Controller:
        icon: unifi.png
        href: https://10.100.10.5:8443
        description: (currently off)
        ping: http://10.100.10.5
        widget:
            type: unifi
            url: https://10.100.10.5:8443
            username: afulton101222
            password: {{ unifi_controller_password }}

    - Proxmox:
        icon: proxmox.png
        href: https://10.100.10.10:8006
        ping: http://10.100.10.10
        widget:
            type: proxmox
            url: https://10.100.10.10:8006
            username: {{ proxmox_api_token_id }}
            password: {{ proxmox_api_key }}
    
    - TrueNAS:
        icon: truenas-scale.png
        href: https://10.100.10.15
        ping: https://10.100.10.15
        widget:
            type: truenas
            url: https://10.100.10.15
            key: {{ truenas_api_key }} # not required if using username / password

- Services:
    - Plex:
        icon: plex.png
        href: https://app.plex.tv/
        widget:
            type: plex
            url: http://10.100.10.15:32400
            key: {{ plex_key }}

    - Jellyseerr:
        icon: jellyseerr.png
        href: http://requests.adfulton.com
        widget:
            type: jellyseerr
            url: http://10.100.10.20:5055
            key: {{ jellyseerr_key }}

    - Radarr:
        icon: radarr.png
        href: http://10.100.10.20:7878/
        widget:
            type: radarr
            url: http://10.100.10.20:7878
            key: {{ radarr_key }}
            enableQueue: false # optional, defaults to false

    - Sonarr:
        icon: sonarr.png
        href: http://10.100.10.20:8989/
        widget:
            type: sonarr
            url: http://10.100.10.20:8989
            key: {{ sonarr_key }}
            enableQueue: false # optional, defaults to false
    
    - Prowlarr:
        icon: prowlarr.png
        href: http://10.100.10.20:9696/
        widget:
            type: prowlarr
            url: http://10.100.10.20:9696
            key: {{ prowlarr_api_key }}
    
    - SABnzbd:
        icon: sabnzbd.png
        href: http://10.100.10.20:8080
        widget:
            type: sabnzbd
            url: http://10.100.10.20:8080
            key: {{ sabnzbd_key }}

    - Onsite Uptime Kuma:
        icon: uptime-kuma.png
        href: https://onsite.adfulton.com
        widget:
            type: uptimekuma
            url: https://onsite.adfulton.com
            slug: default

    - Cloud Uptime Kuma:
        icon: uptime-kuma.png
        href: https://cloud.adfulton.com
        widget:
            type: uptimekuma
            url: https://cloud.adfulton.com
            slug: default

    - Semaphore:
        icon: semaphore.png
        href: http://10.100.10.17:3000


    - Netbox:
        icon: netbox.png
        href: http://10.100.10.20:8081

    
    - Speedtest Tracker:
        icon: si-speedtest.svg-#ADD8E6
        href: https://10.100.10.20:8443
        widget:
            type: speedtest
            url: https://10.100.10.20:8443
